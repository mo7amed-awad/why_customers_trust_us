{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "carlos-meneses/laravel-mpdf": "^2.1",
        "fabpot/goutte": "^4.0",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.10",
        "laravel/sanctum": "^3.3",
        "laravel/telescope": "^5.15",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2.2",
        "nwidart/laravel-modules": "^10.0",
        "pion/laravel-chunk-upload": "^1.5",
        "pusher/pusher-php-server": "^7.2",
        "rap2hpoutre/fast-excel": "^5.3",
        "realrashid/sweet-alert": "^7.0",
        "stevebauman/location": "^6.5",
        "stichoza/google-translate-php": "^5.3",
        "yajra/laravel-datatables-oracle": "^10.10"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.18",
        "laraveldaily/larastarters": "^1.4",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/Functions/WhatsApp.php",
            "app/Functions/Upload.php",
            "app/Functions/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
